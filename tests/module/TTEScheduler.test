%description:
Test Scheduler

%file: package.ned
package test.modul.ttescheduler;

%inifile: test.ini
[General]
#preload-ned-files = *.ned ../../*.ned @../../../../nedfiles.lst
ned-path = .;../../../../src;../../../../../inet/src;../../lib/

network = TTESchedulerTest

sim-time-limit = 1s

#[Cmdenv]
cmdenv-event-banners=false
cmdenv-express-mode=false

#[Parameters]
*.testing=true

*.cli_app.tSend=1s
*.cli_app.sendBytes=100B


%contains: stdout
REGISTRED: ACTION_TIME_EVENT: action_time:1000 NOW [simtime:0; ticks:0; total_ticks:37500]
