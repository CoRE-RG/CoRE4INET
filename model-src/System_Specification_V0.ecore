  <eClassifiers xsi:type="ecore:EClass" name="Devices" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for the devices."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="device" lowerBound="1"
        upperBound="-1" eType="#//Device" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of devices."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Device" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines a device in the network. A device can be a switch or an end system or any combination of both. &#xD;&#xA;The real capabilites of the device are described in the field &quot;capabilities&quot;."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="port" upperBound="-1" eType="#//Port"
        containment="true" eOpposite="#//Port/device">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of specified logical ports. A logical port can be mapped to a physical port, a host port or a internal port (management interface, sync module). &#xD;&#xA;This mapping can be done later in the device target mapping."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="capability" upperBound="-1"
        eType="#//CapabilitiesEnum">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Required capabilities for the device."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Port" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a logical port of a device."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="transmissionSpeed" eType="#//SpeedEnum"
        defaultValueLiteral="1000Mbps">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Transmission speed of the port in bits per second."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="staticRxDelay" eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Used to specify the receive latency of a port, if known at design time. Can be used as input for simulation and scheduling tools."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="staticTxDelay" eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Used to specify the transmit latency of a port, if known at design time. Can be used as input for simulation and scheduling tools."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mediaType" eType="#//MediaType"
        defaultValueLiteral="default"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="device" lowerBound="1"
        eType="#//Device" transient="true" eOpposite="#//Device/port"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Links" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for links in the system specification."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="link" lowerBound="1" upperBound="-1"
        eType="#//Link" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of links in the system specification."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Link" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines a link between two local ports inside the network. &#xD;&#xA;A link represents a directed and unidirectional connection between two ports inside the network or a component."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refSender" ordered="false"
        lowerBound="1" eType="#//Port">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the sending port."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refReceiver" ordered="false"
        lowerBound="1" eType="#//Port">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the receiving port."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cableLength" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigDecimal"
        defaultValueLiteral="0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines the length of the cable, if known at design time. Can be used for scheduling and simulation purposes."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxSpeed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The maximum speed for the link. Usually only one speed is used in TTEthernet networks ."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VirtualLinks" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for defining virtual links."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="virtualLink" lowerBound="1"
        upperBound="-1" eType="#//VirtualLink" containment="true" eKeys="#//VirtualLink/vlid">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of virtual links."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VirtualLink" abstract="true" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a virtual link according to TTEthernet or AFDX."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="destMacAddressCF" lowerBound="1"
        eType="#//CTMarkerType" defaultValueLiteral="00:00:00:00">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Constant field of the destination MAC address for this particular virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="vlid" lowerBound="1" eType="#//VirtualLinkIDType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Identifier of the virtual link. This can be the actual virtual link number as it used inside the Ethernet frames itself, &#xD;&#xA;or a name alias, which can be defined inside the virtual link map."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="jitter" lowerBound="1"
        eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Jitter as defined in ARINC 664 (AFDX)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxE2eLatency" eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum allowed latency for an end-to-end transmission of a frame on this virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="frameRoute" upperBound="3"
        eType="#//FrameRoute" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the frame route. The frame route is calculated by the scheduling tool and does not need to be maintained manually."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refSender" ordered="false"
        lowerBound="1" eType="#//Device">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the sending device of the virtual link."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="attribute"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refReceiver" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Device">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference the list of receiving device of the virtual link."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="alias" upperBound="-1"
        eType="#//Alias" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of aliases for this virtual link. aliases can be used to map between different notations of the virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PCFVirtualLink" eSuperTypes="#//VirtualLink">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines a virtual link used for PCF (protocol control frame) transmissions. &#xD;&#xA;A PCF is a special kind of rate-constrained virtual link, with a fixed size. &#xD;&#xA;PCF links are used by the clock synchronization (see TTEthernet specification)."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataVirtualLink" eSuperTypes="#//VirtualLink">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Base for all data carrying virtual links."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="semantic" eType="#//SemanticEnum"
        defaultValueLiteral="state">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Semantics of the data, which are transferred over this virtual link. &#xD;&#xA;It determines the used buffer types during traversal inside the devices of the network."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxLength" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="1500">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum length of a frame of this virtual link in bytes.&#xD;&#xA;WARNING: The payload length includes the sequence number, because the sequence number is part of the payload"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refPeriod" eType="#//Period">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to requested period for this virtual link. &#xD;&#xA;A time-triggered link will use exactly this period for transmission, &#xD;&#xA;with a precision inside the specified jitter, deadline and latency criteria. &#xD;&#xA;A rate-constrained link will be realized using BAG accounts inside the devices."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="transparentClockEnable"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This enables the transparent clock time stamping mechanism of TTEthernet. &#xD;&#xA;If this is enabled, every TTEthernet device writes the 64-bit transparent clock value inside the bytes 20 to 27 of the Ethernet frame payload.&#xD;&#xA;This can be used for latency measurements of the frames inside the network."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RCVirtualLink" eSuperTypes="#//DataVirtualLink">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Specifies a rate-constrained link (see ARINC 664 Part 7)."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TTVirtualLink" eSuperTypes="#//DataVirtualLink">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Specifies a time-triggered virtual link. &#xD;&#xA;The frames will be submitted after the point in time specified in the field &quot;release&quot; has passed and shall be received before the point in time specified in the field &quot;deadline&quot; has passed."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="release" eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Earliest release time at the sender. &#xD;&#xA;This is the time when the information is ready at the sender."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="deadline" eType="#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Latest receive time for all receivers that receive data on this virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FrameRoute" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Contains a frame route for one non-redundant route through the network.&#xD;&#xA;Usually the frame routes are calculated by a scheduling tool and don't require manual editing."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="segments" lowerBound="1"
        upperBound="-1" eType="#//FrameRouteSegment" containment="true" resolveProxies="false"
        eKeys="#//FrameRouteSegment/hop">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of segemnts of the route."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FrameRouteSegment" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Segment of a non-redundant frame route."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hop" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Number of the hop.The hop count is the number of subsequent steps along the path from source to sink."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refOutgoingLink" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Link">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="References the virtual link for this segment."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Periods" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Contains the periods used in the system specification and all of its derived information."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="period" upperBound="-1"
        eType="#//Period" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of periods."/>
      </eAnnotations>
    </eStructuralFeatures>
