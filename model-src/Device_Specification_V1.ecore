      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Contains the switch configuration."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DeviceConfig" abstract="true" eSuperTypes="http://www.eclipse.org/emf/2002/Ecore#//ENamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Abstract base for device configurations. A device configuration contains all device-specific settings on a hardware independent level. &#xD;&#xA;For hardware-specific configuration, see device target mapping. "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ctMarker" eType="ecore:EDataType System_Specification_V0.ecore#//CTMarkerType"
        defaultValueLiteral="03:00:00:00">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This marker indicates a frame for a virtual link. For a virtual link frame it must be equal to the result of the bit-wise AND of the constant field of the destination MAC address with the ctMask."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ctMask" eType="ecore:EDataType System_Specification_V0.ecore#//CTMarkerType"
        defaultValueLiteral="ff:ff:ff:ff">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This mask is used to identify the bits of the constant field inside the destination MAC address. Those bits designate a frame used inside a virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EndSystemConfig" eSuperTypes="#//DeviceConfig">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This complex type contains general configuration entries for an end system."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tsSource" eType="#//TimeSourceEnum"
        defaultValueLiteral="globalTime">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Source clock used for timestamping frames that are incoming to the end system. &#xD;&#xA;Either the global (synchronized) clock can be used, or the free running clock, &#xD;&#xA;which is just a counter of the clock cycles of the CPU since the start of the system."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="externalClockSynchronization"
        eType="#//ExternalClockSynchronization" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Contains the settings for external clock synchronization configuration of the device."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="sourceMacAddress" eType="ecore:EDataType System_Specification_V0.ecore#//MacAddressType"
        defaultValueLiteral="01:00:00:00:00:00">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Source MAC address of the end system to be used for all sent frames from the host."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="destMacAddressCF" eType="ecore:EDataType System_Specification_V0.ecore#//CTMarkerType"
        defaultValueLiteral="03:00:00:00">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="First 32 bits of the destination MAC address of the end system to be used for all sent frames from the host. See AFDX specification."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="externalSyncAckRequired"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines whether selection of the local synchronization priority requires host action."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ExternalClockSynchronization" eSuperTypes="System_Specification_V0.ecore#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Configuration used, if the end system should do external clock synchronization, i.e. it has an external clock source, to which it tries to synchronize the global clock  (only usable for synchronization masters)."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxExtCorrValue" lowerBound="1"
        eType="ecore:EDataType System_Specification_V0.ecore#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum amount of time to be applied to the clock values for each integration cycle. This can be used to draw the global clock towards the external clock source."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SwitchConfig" eSuperTypes="#//DeviceConfig">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This complex type contains general configuration entries for an switch."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BagAccounts" eSuperTypes="System_Specification_V0.ecore#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for BAG accounts. The BAG accounts are divided into two kinds - incoming and outgoing."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingBagAccount" upperBound="-1"
        eType="#//IncomingBagAccount" containment="true" eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Container for all incoming BAG accounts."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingBagAccount" upperBound="-1"
        eType="#//OutgoingBagAccount" containment="true" eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Container for all outgoing BAG accounts."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BagAccount" abstract="true" eSuperTypes="System_Specification_V0.ecore#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models one BAG account. This BAG account can be used by one or more virtual links."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bag" lowerBound="1" eType="ecore:EDataType System_Specification_V0.ecore#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Bandwidth allocation gap. See ARINC 664 part 7 (AFDX)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="jitter" lowerBound="1"
        eType="ecore:EDataType System_Specification_V0.ecore#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Variation of the transmission point in time for a frame that is designed to be periodic. See ARINC 664 part 7 (AFDX)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="constrainType" eType="#//ConstrainType"
        defaultValueLiteral="frame">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines the parameter the BAG constraints apply to. It is either applied to the number of frames, or to the size of the transmitted frames."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IncomingBagAccount" eSuperTypes="#//BagAccount">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models one BAG account for incoming traffic."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OutgoingBagAccount" eSuperTypes="#//BagAccount">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models one BAG account for outgoing traffic."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VirtualLinkSchedules" eSuperTypes="System_Specification_V0.ecore#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for virtual link schedules."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="vlSchedule" lowerBound="1"
        upperBound="-1" eType="#//VirtualLinkSchedule" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of virtual link schedules."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="scheduleStartAfterSync"
        eType="ecore:EDataType System_Specification_V0.ecore#//TimeType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="If set schedule_start_after_sync specifies the point in time the schedule is resumed/started after synchronization is up.&#xD;&#xA;If not set the schedule will resumed/started at the next action after the next integration_cycle_start"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VirtualLinkSchedule" eSuperTypes="System_Specification_V0.ecore#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models the schedule of one virtual link inside the device. The schedule consists of the routing information within the device as well as timing information.&#xD;&#xA;Limitations: since Version 1.0 we no longer have frame-generations. &#xD;&#xA;For time-triggered traffic several incoming elements can be specified with the multiple of the period, to work around this limitation."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="priority" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EByte"
        defaultValueLiteral="6">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Priority of the virtual link inside the device. This is mainly used for rate-constrained traffic."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refVirtualLink" lowerBound="1"
        eType="ecore:EClass System_Specification_V0.ecore#//VirtualLink" eKeys="System_Specification_V0.ecore#//VirtualLink/vlid">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="References the virtual link in the system specification, whose schedule is described in the container &quot;incoming&quot;."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incoming" lowerBound="1"
        upperBound="-1" eType="#//CTIncoming" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of incoming critical traffic events for this virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Buffers" eSuperTypes="System_Specification_V0.ecore#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Container for buffer descriptions."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="buffer" lowerBound="1"
        upperBound="-1" eType="#//Buffer" containment="true" eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of buffer descriptions used by this device."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Buffer" abstract="true" eSuperTypes="System_Specification_V0.ecore#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Defines one buffer."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FIFOQueue" eSuperTypes="#//Buffer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a FIFO queue buffer of configurable length."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="length" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Length of the FIFO queue in frames."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SingleEntryBuffer" eSuperTypes="#//Buffer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a single entry buffer. It can not be concurrently written and read, and therefore additional synchronization mechanisms are needed to guarantee consistent use of such a buffer."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DualEntryBuffer" eSuperTypes="#//Buffer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a dual entry buffer. Simultaneous reads and writes are possible. However, it must be guaranteed that it is possible to completely write one entry in between two starts of a read operation."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TripleEntryBuffer" eSuperTypes="#//Buffer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a triple entry buffer. This buffer can be written and read without any timing constraints."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Incoming" abstract="true" eSuperTypes="#//FrameRelayDescriptor">
