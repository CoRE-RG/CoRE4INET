      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Contains optional trigger schedules. Trigger schedules enable the user to define events on the timeline, which can be mapped onto a hardware implementation in the device (interrupts, etc.)."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Switch" eSuperTypes="#//DeviceSpecification">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
