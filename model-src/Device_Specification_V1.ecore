      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Container for defining periods. This periods are in addition to those defined in the system specification."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refDeviceMapping" eType="ecore:EClass Device_Target_Mapping_V1.ecore#//DeviceMapping"
        eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="References the device target mapping, which maps the logical device and its parts to the target device (physical representation)."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EndSystem" eSuperTypes="#//DeviceSpecification">
