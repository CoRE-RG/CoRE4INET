      <details key="documentation" value="Describes one incoming event, the incoming port and its dependent outgoing events."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoing" lowerBound="1"
        upperBound="-1" eType="#//Outgoing" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of scheduled outgoing traffic events."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refInPort" ordered="false"
        lowerBound="1" upperBound="-1" eType="ecore:EClass System_Specification_V0.ecore#//Port"
        eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the port, where the incoming traffic is expected."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CTIncoming" abstract="true" eSuperTypes="#//Incoming">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models incoming traffic of a virtual link."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="removeSequenceNumber" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="If set, the device removes the sequence number of the frame. The sequence number is primarily used for redundancy management."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="redundancyFunction" ordered="false"
        eType="#//IntegrityCheck" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Contains information of the selected redundancy function (see AFDX and TTEthernet redundancy management for further information about redundancy management)."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Outgoing" abstract="true" eSuperTypes="#//FrameRelayDescriptor">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models outgoing traffic of a virtual link."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="appendSequenceNumber" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="If set, the device appends a sequence number to the frame before sending it out. This is primarily used for redundant virtual links."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refOutPort" lowerBound="1"
        upperBound="-1" eType="ecore:EClass System_Specification_V0.ecore#//Port"
        eKeys="System_Specification_V0.ecore#//NamedElement/name">
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="References to the outgoing ports in the system specification, for this outgoing event."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RCIncoming" eSuperTypes="#//CTIncoming">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Models a rate-constrained incoming virtual link."/>
    </eAnnotations>
